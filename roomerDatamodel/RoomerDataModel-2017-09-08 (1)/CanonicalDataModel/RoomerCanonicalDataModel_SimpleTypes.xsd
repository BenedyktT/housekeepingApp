	<simpleType name="TelephoneNumberType">
		<annotation>
			<documentation>A type that describes a telephone number (mobile or fixed line).</documentation>
		</annotation>
		<restriction base="string">
			<maxLength value="16"></maxLength>
			<whiteSpace value="collapse"></whiteSpace>
			<pattern value="(\+){0,1}\d+"></pattern>
		</restriction>
	</simpleType>
